generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Team {
  id              Int      @id @default(autoincrement())
  nome            String
  fansBackground  String
  escudo          String
  anoFundacao     Int?
  camisas         String?
  estadioImages   String?
  hino            String?
  titulos         String?
  name            String?
  games           Int?
  wins            Int?
  draws           Int?
  losses          Int?
  points          Int?
  goalsFavor      Int?
  goalsOwn        Int?
  goalsDifference Int?

  @@map("teams")
}